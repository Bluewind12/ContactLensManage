<?xml version="1.0" encoding="utf-8"?>

<ScrollView
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        xmlns:nendsdk="http://schemas.android.com/apk/res-auto"
        android:layout_height="match_parent"
>
    <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

        <TextView
                android:text="@string/contact_info"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/textView12"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="16dp"
                android:textSize="24sp" android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"/>

        <android.support.v7.widget.CardView
                app:cardUseCompatPadding="true"
                android:layout_width="match_parent"
                                            android:layout_height="match_parent" android:layout_marginTop="8dp"
                                            app:layout_constraintTop_toBottomOf="@+id/textView12"
                                            app:layout_constraintStart_toStartOf="parent"
                                            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="16dp"
                                            android:layout_marginEnd="16dp" android:id="@+id/cardView2">

            <android.support.constraint.ConstraintLayout android:layout_width="match_parent"
                                                         android:layout_height="match_parent">

                <TextView
                        android:text="@string/contact_maker"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" android:id="@+id/textView13"
                        android:layout_marginStart="48dp" android:textSize="18sp"
                        app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="8dp"
                        app:layout_constraintTop_toTopOf="parent"/>
                <TextView
                        android:text="@string/not_setting"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" android:id="@+id/makerText"
                        app:layout_constraintTop_toTopOf="@+id/textView13"
                        app:layout_constraintBottom_toBottomOf="@+id/textView13"
                        app:layout_constraintStart_toStartOf="@+id/lensText"
                        android:textSize="18sp"/>
                <TextView
                        android:text="@string/contact_lens"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" android:id="@+id/textView15"
                        app:layout_constraintTop_toBottomOf="@+id/makerText" android:layout_marginTop="8dp"
                        app:layout_constraintStart_toStartOf="@+id/textView13"
                        android:textSize="18sp"/>
                <TextView
                        android:text="@string/contact_type"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView16" android:layout_marginTop="8dp"
                        app:layout_constraintTop_toBottomOf="@+id/lensText"
                        app:layout_constraintStart_toStartOf="@+id/textView15"
                        android:textSize="18sp"/>
                <TextView
                        android:text="@string/not_setting"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/lensText" app:layout_constraintStart_toStartOf="@+id/typeText"
                        app:layout_constraintTop_toTopOf="@+id/textView15"
                        app:layout_constraintBottom_toBottomOf="@+id/textView15" android:textSize="18sp"/>
                <TextView
                        android:text="@string/not_setting"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintStart_toStartOf="@+id/otherText"
                        android:id="@+id/typeText"
                        app:layout_constraintTop_toTopOf="@+id/textView16"
                        app:layout_constraintBottom_toBottomOf="@+id/textView16" android:textSize="18sp"/>
                <TextView
                        android:text="@string/contact_other"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" android:id="@+id/textView19"
                        android:layout_marginTop="8dp"
                        app:layout_constraintTop_toBottomOf="@+id/textView16"
                        app:layout_constraintStart_toStartOf="@+id/textView16"
                        android:textSize="18sp"/>
                <TextView
                        android:text="@string/not_setting"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/otherText" app:layout_constraintStart_toEndOf="@+id/textView19"
                        android:layout_marginStart="32dp"
                        app:layout_constraintTop_toTopOf="@+id/textView19"
                        app:layout_constraintBottom_toBottomOf="parent" android:textSize="18sp"
                        android:layout_marginBottom="8dp"/>
                <ImageView
                        android:layout_width="0dp"
                        android:layout_height="0dp" app:srcCompat="@drawable/lr_icon"
                        android:id="@+id/imageView3"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toStartOf="@+id/textView13"
                        app:layout_constraintTop_toTopOf="@+id/textView13"
                        app:layout_constraintBottom_toBottomOf="@+id/textView13" android:layout_marginEnd="8dp"
                        android:layout_marginStart="8dp" app:layout_constraintVertical_bias="0.39"/>
            </android.support.constraint.ConstraintLayout>
        </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView
                app:cardUseCompatPadding="true"
                android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            app:layout_constraintTop_toBottomOf="@+id/cardView2"
                                            app:layout_constraintStart_toStartOf="parent"
                                            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="16dp"
                                            android:layout_marginEnd="16dp" android:id="@+id/cardView2_2"
                                            android:layout_marginTop="8dp">

            <android.support.constraint.ConstraintLayout android:layout_width="match_parent"
                                                         android:layout_height="match_parent">

                <TextView
                        android:text="@string/contact_maker"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" android:id="@+id/textView13_2"
                        android:layout_marginStart="48dp" android:textSize="18sp"
                        app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="8dp"
                        app:layout_constraintTop_toTopOf="parent"/>
                <TextView
                        android:text="@string/not_setting"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" android:id="@+id/makerText_2"
                        app:layout_constraintTop_toTopOf="@+id/textView13_2"
                        app:layout_constraintBottom_toBottomOf="@+id/textView13_2"
                        app:layout_constraintStart_toStartOf="@+id/lensText_2"
                        android:textSize="18sp"/>
                <TextView
                        android:text="@string/contact_lens"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" android:id="@+id/textView15_2"
                        app:layout_constraintTop_toBottomOf="@+id/makerText_2" android:layout_marginTop="8dp"
                        app:layout_constraintStart_toStartOf="@+id/textView13_2"
                        android:textSize="18sp"/>
                <TextView
                        android:text="@string/contact_type"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView16_2" android:layout_marginTop="8dp"
                        app:layout_constraintTop_toBottomOf="@+id/lensText_2"
                        app:layout_constraintStart_toStartOf="@+id/textView15_2"
                        android:textSize="18sp"/>
                <TextView
                        android:text="@string/not_setting"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/lensText_2" app:layout_constraintStart_toStartOf="@+id/typeText_2"
                        app:layout_constraintTop_toTopOf="@+id/textView15_2"
                        app:layout_constraintBottom_toBottomOf="@+id/textView15_2" android:textSize="18sp"/>
                <TextView
                        android:text="@string/not_setting"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintStart_toStartOf="@+id/otherText_2"
                        android:id="@+id/typeText_2"
                        app:layout_constraintTop_toTopOf="@+id/textView16_2"
                        app:layout_constraintBottom_toBottomOf="@+id/textView16_2" android:textSize="18sp"/>
                <TextView
                        android:text="@string/contact_other"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" android:id="@+id/textView19_2"
                        android:layout_marginTop="8dp"
                        app:layout_constraintTop_toBottomOf="@+id/textView16_2"
                        app:layout_constraintStart_toStartOf="@+id/textView16_2"
                        android:textSize="18sp"/>
                <TextView
                        android:text="@string/not_setting"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/otherText_2" app:layout_constraintStart_toEndOf="@+id/textView19_2"
                        android:layout_marginStart="32dp"
                        app:layout_constraintTop_toTopOf="@+id/textView19_2"
                        app:layout_constraintBottom_toBottomOf="parent" android:textSize="18sp"
                        android:layout_marginBottom="8dp"/>
                <ImageView
                        android:layout_width="0dp"
                        android:layout_height="0dp" app:srcCompat="@drawable/r_icon"
                        android:id="@+id/imageView3_2"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toStartOf="@+id/textView13_2"
                        app:layout_constraintTop_toTopOf="@+id/textView13_2"
                        app:layout_constraintBottom_toBottomOf="@+id/textView13_2" android:layout_marginEnd="8dp"
                        android:layout_marginStart="8dp" app:layout_constraintVertical_bias="0.39"/>
            </android.support.constraint.ConstraintLayout>
        </android.support.v7.widget.CardView>
        <Space
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
                app:layout_constraintTop_toBottomOf="@+id/cardView2_2" android:layout_marginTop="16dp"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="16dp"/>

        <net.nend.android.NendAdView
                android:id="@+id/nend100"
                android:layout_width="320dp"
                android:layout_height="100dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toBottomOf="@id/cardView2_2"
                android:layout_marginBottom="16dp" android:layout_marginTop="16dp"
                nendsdk:NendApiKey="@string/ad_apk_100"
                nendsdk:NendSpotId="@integer/ad_id_100"/>

    </android.support.constraint.ConstraintLayout>
</ScrollView>